    "happy-dom": "20.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "^5.7.0",
    "typescript-eslint": "8.56.1",
