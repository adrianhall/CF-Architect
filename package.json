    "astro": "5.18.0",
    "drizzle-orm": "0.45.1",
    "elkjs": "0.11.0",
    "fflate": "0.8.2",
    "html-to-image": "1.11.11",
    "nanoid": "5.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "4.2.1",
    "zod": "^3.24.0",
    "zustand": "5.0.11"
