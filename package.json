{
  "name": "cf-architect",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "wrangler dev",
    "build": "astro build",
    "check": "run-s format typecheck lint",
