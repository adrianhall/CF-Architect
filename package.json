{
  "name": "cf-architect",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "wrangler dev",
    "prebuild": "run-s typecheck lint format:check",
    "build": "astro build",
    "preview": "astro preview",
    "deploy": "wrangler d1 migrations apply DB --remote && wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.5.0",
    "@astrojs/react": "^4.2.1",
    "@tailwindcss/vite": "^4.1.0",
    "@xyflow/react": "^12.6.0",
    "astro": "^5.5.0",
    "drizzle-orm": "^0.39.0",
    "elkjs": "^0.9.3",
    "nanoid": "^5.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.0",
    "zod": "^3.24.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250224.0",
    "@eslint/js": "9.39.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "drizzle-kit": "^0.30.0",
    "eslint": "9.39.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.6.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "^5.7.0",
    "typescript-eslint": "8.56.1",
    "vitest": "^3.0.0",
    "wrangler": "^4.0.0"
  }
}
